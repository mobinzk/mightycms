@linei:@template"¤2* {
	box-sizing: border-box;
	-webkit-box-sizing: border-box;
	-moz-box-sizing: border-box;
}

body {
	font-family: $OpenSans;
	background: #f0f2f5;
	min-width: 960px;
}

.left {
	float: left;
}

.right {
	float: right;
}

.uim-button {
	font-family: $QuickSand;
	border: 0;
	@include border-radius(5px);
	color: #fff;
	cursor: pointer;
	outline: none;
	text-decoration: none;
}

.green {
	background: #8cc152;
	border-bottom: 2px solid #71a636;
}

.red {
	background: #ed5565;
	border-bottom: 2px solid #da4453;
}

.blue {
	background: #51b2cc;
	border-bottom: 2px solid #429db6;
}

.move {
	padding: 14px 20px;
	float: right;
	border-left: 1px solid #e1e1e1;
	cursor: move;
	background: #F0F2F5;
	@include border-radius(0 5px 5px 0);
}

.collapse {
	float: right;
	border-left: 1px solid #e1e1e1;
	padding: 13px 20px 14px;
	background: #F0F2F5;
	cursor: pointer;
}

.icon-pages {
	@extend .icon-pages;
	display: inline-block;
	width: 16px;
	height: 17px;
	vertical-align: top;
	margin: 10px 10px 10px 15px;
}

.icon-move {
	@extend .icon-move;
	width: 8px;
	height: 9px;
	display: block;
}

.icons {
	text-align: center;
}

.icon-collapse-up {
	@extend .icon-collapse-up;
	width: 10px;
	height: 10px;
	display: block;
}

.icon-collapse-down {
	@extend .icon-collapse-down;
	width: 10px;
	height: 10px;
	display: block;
}

.icon-add {
	@extend .icon-add;
	display: inline-block;
	width: 16px;
	height: 16px;
}

.icon-delete {
	@extend .icon-delete;
	display: inline-block;
	width: 16px;
	height: 16px;
}

.icon-update {
	@extend .icon-update;
	display: inline-block;
	width: 16px;
	height: 13px;
}

.icon-published {
	@extend .icon-published;
	display: inline-block;
	width: 16px;
	height: 16px;
}

.icon-unpublished {
	@extend .icon-unpublished;
	display: inline-block;
	width: 16px;
	height: 16px;
}

.icon-sort {
	@extend .icon-sort;
	display: inline-block;
	width: 16px;
	height: 17px;
}

.icon-login {
	@extend .icon-login;
	display: inline-block;
	width: 12px;
	height: 17px;
}

.icon-logout {
	@extend .icon-logout;
	display: inline-block;
	width: 12px;
	height: 17px;
}

.icon-login-failure {
	@extend .icon-login-failure;
	display: inline-block;
	width: 16px;
	height: 16px;
}

.uim-state-highlight {
	@include border-radius(5px);
	background: #fff8f9;
}

.add-new-item {
	padding: 10px 15px;
	display: inline-block;
}

.actions {
	form {
		display: inline-block;
	}
}

.uim-hidden {
	display: none;
}

.uim-alert {
	padding: 20px;
	text-align: center;
	font-family: $QuickSand;
	cursor: pointer;
	position: relative;

	.heading {
		font-size: 23px;
		margin: 0 0 10px;
		font-weight: 600;
		display: inline-block;
		vertical-align: bottom;
	}

	.message {
		margin-top: 5px;
		font-size: 16px;
	}

	.icon {
		width: 40px;
		height: 40px;
		display: inline-block;
	}

	.close {
		@extend .icon-close;
		width: 25px;
		height: 25px;
		position: absolute;
		right: 30px;
		top: 40px;
	}

	form {
		display: inline-block;

		button {
			font-family: $QuickSand;
			background: none;
			border: none;
			font-size: 16px;
			text-decoration: underline;
			cursor: pointer;
			outline: none;
			padding: 0;
			margin: 0;
		}
	}

	
	a,
	button {
		color: #5e714e;
	}
	:hover {
		text-decoration: none;
	}

}

.uim-success {
	background: #f4faee;
	border-bottom: 2px solid #d0d9c8;
	color: #9aaa8c;

	.icon {
		@extend .icon-success;
	}
}

.uim-failed {
	background: #fbf0f1;
	border-bottom: 2px solid #f7d0d4;
	color: #ed5565;

	.icon {
		@extend .icon-failed;
	}
}

header.main {
	background: #434a54;
	overflow: hidden;

	.logo {
		font-family: $QuickSand;
		font-size: 24px;
		color: #fff;
		font-weight: normal;
		padding: 17px 30px;
		float: left;

		img {
			vertical-align: bottom;
			display: inline-block;
			padding: 0 5px 0 0;
		}

		a {
			color: #fff;
			text-decoration: none;
		}

		span {
			color: #fff;
			vertical-align: bottom;
			display: inline-block;
			margin: 0 0 4px 0;
		}
	}

	nav.utility {
		float: right;

		ul {

			li {
				display: inline-block;

				a {
					color: #7c8695;
					font-weight: 100;
					text-decoration: none;
					padding: 24px 20px;
					display: block;
				}

				:hover {
					background: #393f48;
					color: #fff;
				}
			}

			li.selected {
				a {
					background: #393f48;
					color: #fff;
				}
			}

			li.logout {
				margin: 0 0 0 10px;

				a {
					padding: 24px 30px;
					color: #db4453;
					border-left: 1px solid #616a78;
				}
			}

		}
	}
}

.container {
	padding: 30px;
}

.contents {
	background: #fff;
	@include border-radius(0 0 5px 5px);
	padding: 20px;
	border-bottom: 2px solid #e3e4e8;
}

nav.main {
	display: block;
	background: #f6f8fa;
	@include border-radius(5px 5px 0 0);

	ul {
		li {
			display: inline-block;

			a {
				color: #51b2cc;
				text-decoration: none;
				font-weight: 100;
				padding: 25px 20px;
				display: block;
			}

			:hover {
				color: #ed5565;
			}
		}

		li.selected {
			a {
				border-top: 3px solid #51b2cc;
				background: #fff;
				@include border-radius(5px 5px 0 0);
			}

			:hover {
				color: #51b2cc;
			}
		}
	}	
}

aside.info_box {
	width: 200px;
	background: #fff;
}

.sub-nav,
.sub-nav-main {
	background: #F6F8FA;
	// padding: 10px 20px;
	border-top: 1px solid #E3E4E8;
	border-bottom: 1px solid #E3E4E8;

	li {
		display: inline-block;
		// margin: 0 20px 0 0;
		
		a {
			font-size: 14px;
			font-weight: 600;
			text-decoration: none;
			color: #51B2CC;
			padding: 10px 20px;
			display: block;
		}

		:hover {
			background: #fff;
			// color: #fff;
		}
	}

	li.selected {
		a {
			background: #fff;
			color: #ED5565;
		}
	}
}

.copyright {
	color: #999;
	font-size: 12px;
	text-align: center;
	padding: 30px;
}

ul.uim-form {
	width: 50%;
	display: inline-block;
	vertical-align: top;

	.ui_input_wrapper {
		
		>li {
			padding: 0 0 20px;
			position: relative;

			label {
				display: inline-block;
				// width: 100%;
				font-size: 14px;
				font-weight: 600;
				color: #7C8695;
				padding: 10px 20px 10px 0;
				vertical-align: middle;
			}
			label:after {
				content: attr(data-title);
				color: #ed5565;
			}

			> .uim-button {
				font-size: 12px;
				vertical-align: middle;
			}

			input,
			textarea,
			select {
				display: block;
				width: 100%;
				max-width: 100%;

				font-family: $OpenSans;
				font-size: 14px;
				outline: none;
				padding: 15px;
				border: 2px solid #e3e4e8;
				@include border-radius(5px);
			}

			textarea {
				line-height: 24px;
				@include border-radius(0 0 5px 5px);
			}

			.required,
			.error {
				background: #fff8f9;
				border: 2px solid #ed5565;
			}

			.group {
				background: #F6F8FA;
				padding: 0px 20px 20px;
				@include border-radius(5px);
				border: 1px solid #e3e4e8;
				border-bottom: 2px solid #e3e4e8;

				label {
					margin: 20px 0 0 0;
				}
			}


			ul.checkbox,
			ul.radio {
				margin: 5px 0 0 0;
				li {
					display: inline-block;
					width: 50%;
					input {
						width: auto;
						display: inline-block;
						margin: 0 5px 0 0;
					}

					label {
						width: auto;
						display: inline-block;
						font-weight: normal;
						padding-right: 0;
					}
				}
			}

			.template,
			.new-variation {
				border: 1px solid #e3e4e8;
				border-bottom: 2px solid #e3e4e8;
				@include border-radius(5px);
				display: none;
				margin: 0 0 10px;

				ul {
					padding: 20px 20px 10px;
				}
			}

			.new-variation {
				display: block;
			}

			.variations {
				margin: 20px 0 0 0;

				li {
					padding: 0 0 20px;
					position: relative;
				}
				
				.variation {
					background: #F6F8FA;
					// padding: 0px 20px 20px;
					@include border-radius(5px);
					border: 1px solid #e3e4e8;
					border-bottom: 2px solid #e3e4e8;
					position: relative;
				}

				.variation {
					margin: 10px 0 0 0;
				}

				.uim-state-highlight {
					margin: 13px 0 0 0;
				}

				.item_wrapper {
					border-bottom: 1px solid #e3e4e8;					
				}

				.name {
					padding: 12px 20px 11px;
					display: inline-block;
					font-size: 14px;
					color: #666;
					font-weight: 600;
				}

				.fields {
					padding: 20px 20px 10px;
					background: #fff;
					display: none;
				}


				.actions {
					position: absolute;
					right: 110px;
					top: 8px;

					.uim-button {
						font-size: 12px;
						padding: 5px 10px 4px;
					}

					.delete {
						margin: 0 0 0 5px;
					}
				}

			}

		}
	}

	.save-page {
		display: block;
		width: 100%;
		padding: 10px;
		font-weight: bold;
		font-size: 14px;
	}
}

.uim_file_list {
	max-width: 100%;

	> ul {
		padding: 0 !important;
	}

	li {
		max-width: 100%;
	}

	a {
		display: block;
	}

	.remove {
		width: 100%;
		padding: 10px;
		display: block;
		@include border-radius(0);
		text-align: center;
	}
}

.new-variation {
	>.uim-button {
		display: block;
		width: 100%;
		padding: 10px;
		text-align: center;
		@include border-radius(5px 5px 0 0);
	}
}

.existing_file {
	min-height: 150px;
	max-width: 100%;
	background: #f9f9f9;
	@include border-radius(5px 5px 0 0);
	border: 1px solid #e3e4e8;
	background-position: center top;
	background-repeat: no-repeat;
	background-size: 100%;
	border-bottom: none;

	img {
		max-width: 100%;
	}
}

ul.uim-form 
.ui_input_wrapper > li 
input[type="file"] {
	margin: 0;
	@include border-radius(0 0 5px 5px);
}

.uim-preview {
	width: 45% !important;
	margin: 0 0 0 4%;
	display: inline-block;
	vertical-align: top;
}
.mightypreview {
	top: 0;
	right: -100%;
	position: absolute;
	width: 95%;
	display: none;
}
.mightycontent {
	padding: 15px;
	@include border-radius(5px);
	border: 1px solid #e3e4e8;
	border-bottom: 2px solid #e3e4e8;
	height: 292px;
	background: #fff;
	overflow: scroll;
	margin: 36px 0 0 0;
}

.uim-table-wrapper {
	border: 1px solid #EEE;
	border-bottom: 2px solid #EEE;
	@include border-radius(5px);
	background: #F6F8FA;
	margin: 30px 0;
	overflow: hidden;

	.upload {
		padding: 10px 15px;
		background: #f9f9f9;
		border-bottom: 1px solid #eee;

		label {
			display: inline-block;
			font-size: 14px;
			font-weight: 600;
			color: #7C8695;
			vertical-align: middle;
			padding: 0 10px 0 0;
		}

		.uim-button {
			padding: 10px 30px;
			vertical-align: middle;
		}
	}
}
.uim-table {
	width: 100%;
	font-size: 14px;
	color: #666;
	font-weight: 600;

	tr:nth-child(odd) {
		background: #fff;
	}

	tr {
		border-bottom: 1px solid #e3e4e8;
	}

	tr:last-child {
		border-bottom: none;
	}

	th {
		font-weight: bold;
		border-bottom: 2px solid #e3e4e8;
		padding: 15px 15px;
		@include border-radius(5px 5px 0 0);
		background: #F6F8FA;
	}

	.ui-sortable-helper {
		background: #fff;
	}

	a {
		color: #51B2CC;
	}

	a:hover {
		text-decoration: none;
	}

	.action {
		text-align: right;
		padding: 5px 15px;

		a {
			padding: 5px 10px;
			font-size: 10px;
			color: #fff;
		}
	}

	.time {
		width: 200px;
		text-align: center;
	}

	td {
		padding: 15px 15px;
	}

	button {
		padding: 5px 10px;
	}

	form {
		display: inline-block;
	}

	.pimage {
		max-width: 100px;
		max-height: 100px;
	}

	.move {
		padding: 19px 20px;
		margin: -8px -15px -8px 10px;
		height: 44px;
		@include border-radius(0);
	}
}

.credits {
	color: #51B2CC;
}

.credits:hover {
	text-decoration: none;
}

.mighty-editor-tools {
	background: #E3E4E8;
	// padding: 10px 15px;
	@include border-radius(5px 5px 0 0);
	padding: 0 !important;

	ul {
		padding: 0 !important;
	}

	li {
		cursor: pointer;
		display: inline-block;
		vertical-align: bottom;
		padding: 10px 15px !important;

		span {
			display: block;
		}

		ul,li {
			padding: 0 !important;
		}
	}

	li + li {
		margin-left: -4px;
	}

	li:hover {
		background-color: #eee;
	}

	#mightyBold {

		span {
			@extend .icon-bold;
			width: 12px;
			height: 15px;
		}
	}

	#mightyBold:hover {
		@include border-radius(5px 0 0 0);
	}

	#mightyItalic {

		span {
			@extend .icon-italic;
			width: 9px;
			height: 15px;
		}
		margin-right: 20px;
	}

	#mightyBullet {

		span {
			@extend .icon-list;
			width: 17px;
			height: 15px;
		}
	}

	#mightyNumbers {

		span {
			@extend .icon-lnumber;
			width: 17px;
			height: 15px;
		}
		margin-right: 20px;
	}

	#mightyLink {

		span {
			@extend .icon-link;
			width: 14px;
			height: 15px;
		}
	}

	#mightyImage {

		span {
			@extend .icon-image;
			width: 18px;
			height: 15px;
		}
	}

	#mightyVideo {

		span {
			@extend .icon-video;
			width: 23px;
			height: 15px;
		}
		margin-right: 20px;
	}

	#mightyH {
		span {
			@extend .icon-header;
			width: 17px;
			height: 15px;
			display: block;
		}

		position: relative;
		ul {
			display: none;
			position: absolute;
			top: 35px;
			left: 0;
			background: #E3E4E8;
			@include border-radius(0 0 5px 5px);
			width: 302px;

			li {
				padding: 10px 14px !important;
				margin: 0;
				font-weight: bold;
				font-size: 14px;
				color: #434a54;
				display: inline-block;
			}

			li:first-child {
				@include border-radius(0 0 0 5px);
			}

			li:last-child {
				@include border-radius(0 0 5px 0);
			}
		}
	}

	#mightyH:hover {
		ul {
			display: block;
		}
	}
